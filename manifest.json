{
  "name": "Cryptospora",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Encrypted Diaspora Messaging",
  "permissions": [
   "tabs", "proxy","background",  "http://*/*", "https://*/*"
],
 "content_scripts": [ {
    "js": [ "bootstrap/js/jquery.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "options_page": "options.html",
"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "inbox.html" 
 }
}
