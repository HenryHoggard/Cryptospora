{
  "manifest_version": 2,

  "name": "Create Message",
  "description": "Create Message for Diaspora",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "sendmessage.html"
  },
 "permissions": [
   "tabs", "proxy","background",  "http://*/*", "https://*/*"
],
 "content_scripts": [ {
    "js": [ "bootstrap/js/jquery.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
